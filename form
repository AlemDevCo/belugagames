<!DOCTYPE html>
<html>
  <head>
    <title>Discord Webhook Tutorial</title>
  </head>
  <body>
    <label for="name">Name</label><br>
             <input type="text" class="name" name="name" placeholder="enter here"id="name"><br>
             
             
 <label for="email">Email</label><br>
             <input type="text" class="email" name="email" placeholder="enter here"id="email"><br>
            
 <label for="message">Message</label><br>
             <input type="text" class="message" name="message" placeholder="enter here"id="message"><br>
            
            
            
             <input type="submit" value="Submit"id="submit-b"onclick="sendMessage()">
  </body>

  <script>
    function sendMessage() {
      var name= document.getElementById("name").value;
var email= document.getElementById("email").value;
var message= document.getElementById("message").value;
      const request = new XMLHttpRequest();
request.open("POST", "https://discord.com/api/webhooks/972836891932127272/jpO-ch4JqzS8GAnM3R-vZ0f99yCN-ihPkduJQRT4Y6r01dtsLYyFbCImHjsuk__40C2R");
request.setRequestHeader('Content-type', 'application/json');
const params = {
  "content": `This is a message`,
  "embeds": [
    {
      "type": "rich",
      "title": `Some form`,
      "description": `Form`,
      "fields": [
        {
          "name": `Username`,
          "value": `${name}`
        },
        {
          "name": `Email`,
          "value": `${email}`
        },
        {
          "name": `Message`,
          "value": `${message}`
        }
      ]
    }
  ]
}
request.send(JSON.stringify(params));
    }
  </script>
</html>
